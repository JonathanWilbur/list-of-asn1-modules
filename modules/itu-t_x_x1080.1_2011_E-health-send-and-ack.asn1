}

Ack ::= CHOICE {
  ok     NULL,
  error
    SEQUENCE {code       ERROR.&code({Simple-Message-Errors}),
              parameter  ERROR.&Parameter({Simple-Message-Errors}{@.code})
  }
}

Simple-Message-Errors ERROR ::= {Simple-Messages.&Errors}

Response ::= SEQUENCE {
